{
  "name": "barn-labs-landing-page",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run copy-pwa && npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "wrangler dev src/worker.ts --local",
    "dev:backend:remote": "wrangler dev src/worker.ts --remote",
    "dev:remote": "npm run copy-pwa && npm-run-all --parallel dev:frontend dev:backend:remote",
    "build": "npm run copy-pwa && vite build && npm run build:worker",
    "build:worker": "esbuild src/worker.ts --format=esm --minify --target=es2020 --bundle --outfile=dist/worker.js --platform=browser --external:node:fs/promises --external:node:child_process --external:node:fs --external:node:path",
    "deploy": "npm run build && wrangler deploy",
    "reset:admin": "tsx scripts/reset-admin-password.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:cli": "python scripts/test_cli.py",
    "generate-overview": "python3 scripts/generate_overview.py",
    "copy-pwa": "cp public/pwa-*.png dist/ 2>/dev/null || echo 'PWA icons copied'",
    "db:reset": "wrangler d1 execute barnlabs-credentials --local --command 'DROP TABLE IF EXISTS users; DROP TABLE IF EXISTS assets; DROP TABLE IF EXISTS shares; DROP TABLE IF EXISTS migrations;'"
  },
  "overrides": {
    "three-mesh-bvh": "0.8.0",
    "three": "0.172.0"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.0",
    "@fontsource-variable/merriweather": "^5.1.0",
    "@fontsource-variable/merriweather-sans": "^5.1.0",
    "@fontsource/galdeano": "^5.1.0",
    "@fontsource/inter": "^5.2.6",
    "@fontsource/poppins": "^5.1.0",
    "@fontsource/sora": "^5.2.6",
    "@fontsource/source-sans-pro": "^5.1.0",
    "@gltf-transform/core": "^4.2.1",
    "@gltf-transform/functions": "^4.2.1",
    "@google/generative-ai": "^0.17.1",
    "@react-email/render": "^1.2.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/xr": "^6.6.20",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-table": "^8.21.3",
    "bcryptjs": "^3.0.2",
    "dompurify": "^3.1.6",
    "draco3dgltf": "^1.5.7",
    "framer-motion": "^12.23.12",
    "glob": "^11.0.3",
    "hono": "^4.5.11",
    "jose": "^5.6.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.441.0",
    "marked": "^14.1.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-image-crop": "^11.0.0",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.14.1",
    "resend": "^6.0.0",
    "three": "0.172.0",
    "three-stdlib": "^2.24.0",
    "draco3d": "^1.5.7",
    "@webxr-input-profiles/motion-controllers": "^1.0.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.0",
    "@cloudflare/workers-types": "^4.20250803.0",
    "@eslint/js": "^9.32.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/draco3dgltf": "^1.4.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/jszip": "^3.4.0",
    "@types/node": "^22.5.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.178.1",
    "@types/webxr": "^0.5.22",
    "@gltf-transform/cli": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.25.4",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^24.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "sourcemap-codec": "^1.4.8",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.27.0"
  }
}
